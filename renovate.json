{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["(^|/)Dockerfile$"],
      "matchStrings": ["ENV TURBO_VERSION=(?<currentValue>.*?)\\n"],
      "depNameTemplate": "turbo",
      "datasourceTemplate": "npm"
    }
  ]
}
